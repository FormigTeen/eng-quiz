{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "singularName": "quiz",
    "pluralName": "quizzes",
    "displayName": "Quiz",
    "description": "Quizzes de futebol"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": false
    },
    "status": {
      "type": "enumeration",
      "enum": ["draft", "published", "archived"],
      "default": "draft"
    },
    "questions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::question.question",
      "mappedBy": "quizzes"
    }
  }
}


